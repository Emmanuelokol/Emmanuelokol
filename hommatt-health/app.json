{
  "expo": {
    "name": "hommat Health",
    "slug": "hommat-health",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hommatt.health"
    },
    
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "882f73af-e1c8-4b6e-b051-c2d7d63c987e"
      }
    }
  }
}
